{
  "name": "k1",
  "version": "1.0.0",
  "description": "Dead-simple Hugo theme with Netlify CMS integration. Intended to be a starter for creating your own theme with an out-of-the-box Netlify CMS integration.",
  "main": "tailwind.config.js",
  "dependencies": {
    "concurrently": "^7.2.1"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.2",
    "autoprefixer": "^10.4.7",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.1.0",
    "tailwindcss": "^3.0.24"
  },
  "scripts": {
    "start": "concurrently npm:watch:*",
    "watch:tw": "tailwindcss -i ./static/css/main.css -o ./static/css/style.css --watch",
    "watch:hugo": "hugo serve",
    "build": "hugo --minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kngjnssy/k1.git"
  },
  "keywords": [],
